<?xml version="1.0" encoding="utf-8"?>
<appSettings>
	<add key="webpages:Version" value="3.0.0.0" />
	<add key="webpages:Enabled" value="false" />
	<add key="ClientValidationEnabled" value="true" />
	<add key="UnobtrusiveJavaScriptEnabled" value="true" />

	<!--服务器端口号-->
	<add key="port" value="7001" />

	<!--数据库类别 mysql或sqlserver 一定要检查!!!-->
	<add key="DBProvider" value="sqlserver" />

	<!--！！！有两种方式连接sqlserver数据库，默认采用第一种方式，可先直接运行，连接不上再安装SqlLocalD，下载文件中有安装文件（以sqllocaldb连接方式连接sqlserver需要安装SqlLocalD_2014文件夹中）-->
	<!--1:以sqllocaldb连接方式连接sqlserver数据库，可能需要安装SqlLocalD-->
	<!---->
	<add key="ConnectionString" value="Data Source=(LocalDB)\MSSQLLocalDB; Initial Catalog=YoursLC; Integrated Security=True;Connect Timeout=30;  AttachDbFilename=%CONTENTROOTPATH%\\App_Data\\YoursLC_2409.mdf" />

	<!--2:一般连接方式(需安装sqlserver并附加数据库文件)-->
	<!--<add key="ConnectionString" value="Server=YDT;Database=YoursLC_2409;User Id=sa;Password=200; Timeout=30;" />-->
	

	<!--使用mysql数据库时必须与ConnectionString中的database=的数据库名一致，否则判断数据表是否存在及数据表存在的字段时会出错 切记！！！ think9_demo_240824 think9_temp_2408 think9_warehouse_2409-->
	<!---->
	<add key="DBase" value="think9_temp_2408" />

	<!--mysql连接字符-->
	<!--<add key="ConnectionString" value="server=127.0.0.1;port=3306;database=think9_temp_2408;user=root;password=ydt13139876200;" />-->

