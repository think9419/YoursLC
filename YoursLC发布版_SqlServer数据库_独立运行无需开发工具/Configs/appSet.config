	<!--默认的上传图片类型-->
	<add key="UploadImgType" value="jpg|png|gif|bmp|jpeg|svg" />

	<!--数据删除时不删除图片，添加了录入表编码，删除数据时不删除图片附件-->
	<add key="DelNoImg" value="tb_tbxx1|tb_tbxx2" />
